{
  "name": "openapi-to-mcp",
  "version": "0.1.4",
  "description": "Model Control Protocol generator from OpenAPI/Swagger specifications",
  "main": "dist/openapi-to-mcp/index.js",
  "types": "dist/openapi-to-mcp/index.d.ts",
  "bin": {
    "openapi-to-mcp": "dist/openapi-to-mcp/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/openapi-to-mcp/index.js",
    "start": "ts-node openapi-to-mcp/index.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "test": "npm run build && node dist/openapi-to-mcp/index.js --help"
  },
  "keywords": [
    "mcp",
    "swagger",
    "openapi",
    "ai",
    "llm",
    "agents",
    "prompt"
  ],
  "author": "Rustho",
  "license": "ISC",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md"
  ]
}
